generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String    @id @default(cuid())
  email     String    @unique
  name      String
  climbs    Climb[]   @relation("UserClimbs")
  createdAt DateTime  @default(now())
}

model Climb {
  id        String    @id @default(cuid())
  name      String
  grade     String
  setterId  String
  setter    User      @relation("UserClimbs", fields: [setterId], references: [id])
  createdAt DateTime  @default(now())
}